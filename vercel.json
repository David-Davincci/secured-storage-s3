{
  "version": 2,
  "buildCommand": "cd api && npm run build",
  "installCommand": "cd api && npm install",
  "builds": [
    {
      "src": "api/src/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/src/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/src/index.js"
    }
  ]
}
