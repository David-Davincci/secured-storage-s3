{
  "name": "secured-storage-api",
  "version": "2.0.0",
  "description": "Secured Storage API - Encrypted file sharing with PostgreSQL",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"npx @babel/node src\"",
    "build": "npx @babel/cli src -s -D -d dist",
    "start": "node dist",
    "prestart": "npm run -s build",
    "test": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "author": "toan@tabvn.com",
  "license": "ISC",
  "dependencies": {
    "archiver": "^2.1.1",
    "aws-sdk": "^2.286.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "dotenv": "^17.2.3",
    "express": "^4.16.2",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "mongodb": "^2.2.36",
    "morgan": "^1.9.0",
    "multer": "^1.3.1",
    "multer-s3": "^2.7.0",
    "node-rsa": "^1.1.1",
    "nodemailer": "^4.6.7",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "react-moment": "^0.7.9",
    "sequelize": "^6.37.7",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.3",
    "@babel/core": "^7.28.5",
    "@babel/node": "^7.28.0",
    "@babel/preset-env": "^7.28.5",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.9.0",
    "nodemon": "^1.19.4"
  }
}
